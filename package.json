    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.5.0",
    "jest-junit": "^6.4.0",
    "node-sass": "^4.12.0",
    "react-test-renderer": "^16.8.4",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "svg-sprite-loader": "^4.1.6",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  }
